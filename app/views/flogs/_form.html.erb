<%= form_for :flog, url: flogs_path do |f| %>
  <div class="field">
    <%= f.label :date%><br />
    <%= f.date_field :date %>
  </div>
  <div class="field">
    <%= f.label :location%><br />
    <%= f.text_field :location %>
  </div>
  <div class="field">
    <%= f.label :who %><br />
    <%= f.text_field :who %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>
  <%= f.submit 'F.LOG IT' %>
<% end %>
